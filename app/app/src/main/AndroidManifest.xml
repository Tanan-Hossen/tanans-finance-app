<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tananfinance">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="false"
        android:label="Tanan's finance"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/Theme.TanansFinance">
        <activity android:name=".MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="com.tananfinance.receiver.WeeklyAlarmReceiver" android:exported="false" />
        <receiver android:name="com.tananfinance.receiver.BootReceiver" android:exported="false">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
    </application>
</manifest>
